<!DOCTYPE html>
<html>
<head>
  <title><%= yield :title %></title>
  <!-- IE stuff -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <!--[if lt IE 9]>
      <%= javascript_include_tag "html5shiv/dist/html5shiv.min.js" %>
    <![endif]-->
  <!-- IE stuff -->

  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag 'parcc', media: 'all', 'data-turbolinks-track' => true %>
</head>
<body>
  <div class="container">
    <%= render "layouts/application/header" %>
    <%= yield %>
    <%= render "layouts/application/logos" %>
  </div>
  <%= render "layouts/application/footer" %>

  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <% if Rails.env.production? %>
    <%= render "shared/google_analytics" %>
  <% end %>
  <script>
    var $buoop = {c:2};
    function $buo_f(){
     var e = document.createElement("script");
     e.src = "//browser-update.org/update.min.js";
     document.body.appendChild(e);
    };
    try {document.addEventListener("DOMContentLoaded", $buo_f,false)}
    catch(e){window.attachEvent("onload", $buo_f)}
  </script>
</body>
</html>
